{
    "name": "tictactoe/tictactoe-php",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "filp/whoops": "^2.14",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "TicTacToe\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}